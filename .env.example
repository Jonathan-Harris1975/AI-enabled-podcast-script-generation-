# Server
PORT=3000
NODE_ENV=development

# LLM provider (OpenAI by default)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
# Set MOCK_AI=true to return deterministic mocked text (for local/dev)
MOCK_AI=false

# RSS default (used if client omits rssUrl in /main)
DEFAULT_RSS_URL=https://venturebeat.com/feed/

# Cache TTL seconds (for session parts)
SESSION_TTL_SECONDS=3600
